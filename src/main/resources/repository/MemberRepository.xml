<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="msa.member.v1.member.dao.MemberDao">
    <select id="login" parameterType="msa.member.v1.member.dto.MemberGetDto" resultType="msa.member.v1.member.model.Member">
        SELECT *, COUNT(*) FROM mem_bsc WHERE LOGIN_ID = #{loginId} AND ENC_PW = #{encPw}
    </select>
</mapper>